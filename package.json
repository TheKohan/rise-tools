{
  "name": "rnt",
  "license": "Apache-2.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "native": "cd apps/rise-mobile && yarn start",
    "ios": "cd apps/rise-mobile && yarn ios",
    "serverw": "yarn workspace @react-native-templates/rise-server",
    "expow": "yarn workspace @react-native-templates/mobile",
    "watch": "yarn workspaces foreach -pi run watch",
    "vercel:install": "yarn set version 3.5 && yarn install",
    "web": "yarn build && cd apps/next && yarn next",
    "web:extract": "DISABLE_EXTRACTION=false yarn workspace next-app dev",
    "web:prod": "yarn workspace next-app build",
    "web:prod:serve": "yarn workspace next-app serve",
    "convert-videos": "cd apps/server && yarn convert-videos",
    "fix": "manypkg fix",
    "postinstall": "yarn check-deps && yarn build",
    "build": "yarn workspaces foreach --exclude next-app run build",
    "upgrade:tamagui": "yarn up '*tamagui*'@latest '@tamagui/*'@latest react-native-web-lite@latest",
    "upgrade:tamagui:canary": "yarn up '*tamagui*'@canary '@tamagui/*'@canary react-native-web-lite@canary",
    "check-deps": "check-dependency-version-consistency .",
    "build-ios-device": "cd apps/rise-mobile; eas build -p ios -e dev-device",
    "build-ios-preview": "cd apps/rise-mobile; eas build -p ios -e preview",
    "build-ios-production": "cd apps/rise-mobile; eas build -p ios -e production --auto-submit",
    "server": "yarn workspace @react-native-templates/rise-server start:dev"
  },
  "resolutions": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-native-svg": "14.1.0",
    "react-native-web": "~0.19.6",
    "@expo/config-plugins": "7.8.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.18.9",
    "@manypkg/cli": "^0.19.1",
    "check-dependency-version-consistency": "^3.0.3",
    "eslint": "^8.21.0",
    "expo": "50",
    "node-gyp": "^9.3.1",
    "prettier": "^2.7.1",
    "react-native-draggable-flatlist": "^4.0.1",
    "turbo": "^1.10.3",
    "typescript": "^5.1.3"
  },
  "packageManager": "yarn@3.6.3"
}
